{
  "name": "@tao.js/socket.io",
  "version": "0.7.0",
  "description": "socket.io middleware to run tao.js seamlessly on server & client",
  "homepage": "https://tao.js.org/",
  "repository": "https://github.com/zzyzxlab/tao.js/tree/master/packages/tao-socket-io",
  "main": "lib",
  "module": "dist",
  "bundles": {
    "browser": "bundles/browser.umd.js"
  },
  "files": [
    "lib",
    "dist",
    "bundles"
  ],
  "scripts": {
    "build:clean": "rimraf lib",
    "build:transpile": "babel src -d lib --source-maps --copy-files",
    "build": "npm run build:clean && npm run build:transpile",
    "rollup:clean": "rimraf dist lib bundles",
    "rollup:build": "rollup --config",
    "rollup": "npm run rollup:clean && npm run rollup:build"
  },
  "author": "eudaimos",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tao.js/core": "^0.6.2"
  },
  "devDependencies": {
    "@tao.js/core": "file:../tao"
  }
}
