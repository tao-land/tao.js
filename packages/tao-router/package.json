{
  "name": "@tao.js/router",
  "version": "0.7.0",
  "description": "connects url routing with tao.js",
  "homepage": "https://tao.js.org/",
  "repository": "https://github.com/zzyzxlab/tao.js/tree/master/packages/tao-router",
  "main": "lib",
  "module": "dist",
  "browser": "bundles/browser.umd.js",
  "bundles": {
    "browser": "bundles/browser.umd.js"
  },
  "files": [
    "lib",
    "dist",
    "bundles"
  ],
  "scripts": {
    "build:clean": "rimraf lib",
    "build:transpile": "babel src -d lib --source-maps --copy-files",
    "build": "npm run build:clean && npm run build:transpile",
    "rollup:clean": "rimraf dist lib bundles",
    "rollup:build": "rollup --config",
    "rollup": "npm run rollup:clean && npm run rollup:build"
  },
  "author": "eudaimos",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tao.js/core": "^0.6.2"
  },
  "devDependencies": {
    "@tao.js/core": "file:../tao"
  },
  "dependencies": {
    "get-value": "^3.0.1",
    "history": "^4.7.2",
    "path-to-regexp": "^2.4.0",
    "routington": "^1.0.3",
    "set-value": "^3.0.0",
    "url-pattern": "^1.0.3"
  }
}
